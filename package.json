{
  "name": "@keyframes/core",
  "version": "1.0.7",
  "description": "Keyframes allows dynamic generation of CSS3 keyframes with callback events and other niceness.",
  "main": "src/keyframes.js",
  "scripts": {
    "build": "npm run es6 && npm run browserify && npm run minify",
    "es6": "babel src/keyframes.js -o dist/keyframes.es6.js --no-comments",
    "browserify": "browserify dist/keyframes.es6.js -o dist/keyframes.js -t [ babelify --presets [ @babel/preset-env ] ]",
    "minify": "uglifyjs --compress --mangle -- dist/keyframes.js > dist/keyframes.min.js",
    "cleanup": "rm -f dist/*.tmp-*",
    "buildExample": "npm run es6Example && npm run browserifyExample && npm run cleanupExample",
    "es6Example": "babel example/example.js -o example/example.es6.js --no-comments",
    "browserifyExample": "browserify example/example.es6.js -o example/example.dist.js -t [ babelify --presets [ @babel/preset-env ] ]",
    "cleanupExample": "rm example/example.es6.js && rm -f *.tmp-*",
    "watch": "nodemon --watch example/example.js --watch src --exec 'npm run build && npm run buildExample'",
    "versions": "browserify --version && babel --version && node --version",
    "test": "npm run es6 && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyframes/Keyframes.git"
  },
  "keywords": [
    "animation",
    "keyframes",
    "css3"
  ],
  "author": "krazyjakee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Keyframes/Keyframes/issues"
  },
  "homepage": "https://github.com/Keyframes/Keyframes",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.38",
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "babelify": "github:ylemkimon/babelify",
    "browserify": "^16.0.0",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.12",
    "uglify-js": "^3.3.5"
  }
}
