{
  "name": "@keyframes/core",
  "version": "2.0.8",
  "description": "Keyframes allows dynamic generation of CSS3 keyframes with callback events and other niceness.",
  "main": "dist/keyframes.js",
  "scripts": {
    "prepare": "npm run buildMain",
    "build": "npm run buildMain && npm run buildExample",
    "buildMain": "bash scripts/build.sh",
    "buildExample": "bash scripts/buildExample.sh",
    "watchExample": "nodemon --watch example/example.ts --watch src --exec \"npm run buildMain && npm run buildExample\"",
    "versions": "browserify --version && tsc --version && node --version",
    "test": "mocha test/bootstrap.js --recursive test",
    "watch": "npm run buildMain && mocha test/bootstrap.js --recursive test --watch",
    "debug": "npm run buildMain && mocha --inspect-brk test/bootstrap.js --recursive test --watch",
    "lint": "eslint --fix . --ext ts",
    "docs": "typedoc --mode file --out docs src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyframes/Keyframes.git"
  },
  "keywords": [
    "animation",
    "keyframes",
    "css3",
    "html5"
  ],
  "author": "krazyjakee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Keyframes/Keyframes/issues"
  },
  "homepage": "https://github.com/Keyframes/Keyframes",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "browserify": "^16.5.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "mocha-jsdom": "^2.0.0",
    "nodemon": "^1.19.4",
    "puppeteer": "^2.0.0",
    "simple-headless-chrome": "^4.3.10",
    "tsify": "^4.0.1",
    "typedoc": "^0.15.2",
    "typescript": "^3.7.2",
    "uglify-js": "^3.6.9",
    "zombie": "^6.1.4"
  },
  "dependencies": {
    "add-px-to-style": "^1.0.0",
    "hyphenate-style-name": "^1.0.3"
  }
}
