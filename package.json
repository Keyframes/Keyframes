{
  "name": "@keyframes/core",
  "version": "2.0.2",
  "description": "Keyframes allows dynamic generation of CSS3 keyframes with callback events and other niceness.",
  "main": "dist/keyframes.js",
  "scripts": {
    "prepare": "mkdir -p dist && npm run build",
    "build": "npm run buildMain && npm run buildExample",
    "buildMain": "npm run es5 && npm run browserify && npm run minify",
    "es5": "tsc",
    "browserify": "browserify src/keyframes.ts -o dist/keyframes.dist.js -p [ tsify --noImplicitAny ]",
    "minify": "uglifyjs --compress --mangle -- dist/keyframes.js > dist/keyframes.min.js",
    "cleanup": "rimraf dist/*.tmp-*",
    "buildExample": "npm run browserifyExample && npm run cleanup",
    "browserifyExample": "browserify example/example.ts -o example/example.dist.js -p [ tsify --noImplicitAny ]",
    "watch": "nodemon --watch example/example.ts --watch src --exec 'npm run build && npm run buildExample'",
    "versions": "browserify --version && tsc --version && node --version",
    "test": "npm run buildMain && mocha test/bootstrap.js --recursive test",
    "lint": "eslint --fix . --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Keyframes/Keyframes.git"
  },
  "keywords": [
    "animation",
    "keyframes",
    "css3",
    "html5"
  ],
  "author": "krazyjakee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Keyframes/Keyframes/issues"
  },
  "homepage": "https://github.com/Keyframes/Keyframes",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "browserify": "^16.5.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "puppeteer": "^2.0.0",
    "simple-headless-chrome": "^4.3.10",
    "tsify": "^4.0.1",
    "typescript": "^3.7.2",
    "uglify-js": "^3.6.9",
    "zombie": "^6.1.4"
  },
  "dependencies": {
    "add-px-to-style": "^1.0.0",
    "hyphenate-style-name": "^1.0.3"
  }
}
